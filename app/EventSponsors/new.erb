<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @eventsponsors.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @eventsponsors.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="eventsponsors[type]" class="fieldLabel">Type</label>
            <input type="text" id="eventsponsors[type]" name="eventsponsors[type]" <%= placeholder("Type") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="eventsponsors[name]" class="fieldLabel">Name</label>
            <input type="text" id="eventsponsors[name]" name="eventsponsors[name]" <%= placeholder("Name") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="eventsponsors[image]" class="fieldLabel">Image</label>
            <input type="text" id="eventsponsors[image]" name="eventsponsors[image]" <%= placeholder("Image") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="eventsponsors[text]" class="fieldLabel">Text</label>
            <input type="text" id="eventsponsors[text]" name="eventsponsors[text]" <%= placeholder("Text") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
