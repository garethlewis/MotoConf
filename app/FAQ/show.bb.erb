<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @faq.id %></h1>
    <%= link_to "Edit", :action => :edit, :id => @faq.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">Id</td>
        <td class="itemValue"><%= @faq.id%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Question</td>
        <td class="itemValue"><%= @faq.question%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Answer</td>
        <td class="itemValue"><%= @faq.answer%></td>
      </tr>
      
    </table>
  </div>

</div>
