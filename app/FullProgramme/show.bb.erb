<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @fullprogramme.id %></h1>
    <%= link_to "Edit", :action => :edit, :id => @fullprogramme.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">Id</td>
        <td class="itemValue"><%= @fullprogramme.id%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Heading</td>
        <td class="itemValue"><%= @fullprogramme.heading%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Body</td>
        <td class="itemValue"><%= @fullprogramme.body%></td>
      </tr>
      
    </table>
  </div>

</div>
